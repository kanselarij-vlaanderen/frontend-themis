{
  "name": "frontend-themis",
  "version": "1.0.0",
  "private": true,
  "description": "Discover data of the Department Kanselarij of the Flemish Government",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanselarij-vlaanderen/frontend-themis.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "release": "release-it",
    "proxy": "ember serve --proxy=https://themis-test.vlaanderen.be",
    "start": "ember serve --proxy=http://localhost:80",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "fastbootDependencies": [
    "crypto"
  ],
